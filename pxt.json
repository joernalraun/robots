{
    "name": "Lofi Robot",
    "description": "Lofi Robot Cardboard Extension",
    "dependencies": {
        "core": "*",
        "v3": "*",
        "bluetooth": "*"
    },
    "files": [
        "main.blocks",
        "main.ts",
        "README.md"
    ],
    "targetVersions": {
        "branch": "master",
        "tag": "v7.1.29",
        "commits": "https://github.com/microsoft/pxt-calliope/commits/6e68cfad115e1c301f4c9892f6788f7bf302368c",
        "target": "7.1.29",
        "pxt": "11.4.16"
    },
